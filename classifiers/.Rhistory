p=read.csv("A:/sem8/final year project/g1.csv")
y1=read.csv("A:/sem8/final year project/m3.csv")
View(p)
str(p)
summary(p)
colnames(p)
library(e1071)
Training <- p
Testing <- y1
nrow(Training)
nrow(Testing)
View(Training)
View(Testing)
library(rminer)
Classifier_1=naiveBayes(t~.,Training)
print(Classifier_1)
Bayes_Prediction_1=predict(Classifier_1,Testing)
print(Bayes_Prediction_1)
class(Testing)
if(Bayes_Prediction_1=='a'){
print("The disease is Alternaria leaf spot")
}else if(Bayes_Prediction_1=='m'){
print("The disease is Myrothecium leaf spot")
}else if(Bayes_Prediction_1=='c'){
print("The disease is Cercospora leaf spot")
} else {
print("The disease is not identified")
}
